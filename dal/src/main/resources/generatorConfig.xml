<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
      <context id="HSQL" targetRuntime="MyBatis3">
        <property name="autoDelimitKeywords" value="true" />
        <property name="beginningDelimiter" value="`" />
        <property name="endingDelimiter" value="`" />


        <jdbcConnection driverClass="org.hsqldb.jdbcDriver"
                        connectionURL="jdbc:hsqldb:mem:jpet"
                        userId="sa"
                        password="">
        </jdbcConnection>
        <javaModelGenerator targetPackage="com.github.zackwoo.scaffolding.entity" targetProject="src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="com.github.zackwoo.scaffolding.mapper"
                         targetProject="src/main/resources">
        </sqlMapGenerator>
        <javaClientGenerator targetPackage="com.github.zackwoo.scaffolding.mapper"
                             targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>


          <table tableName="supplier"></table>
          <table tableName="signon"></table>
          <table tableName="account"></table>
          <table tableName="profile"></table>
          <table tableName="bannerdata"></table>
          <table tableName="orders"></table>
          <table tableName="orderstatus"></table>
          <table tableName="lineitem"></table>
          <table tableName="category"></table>
          <table tableName="product"></table>
          <table tableName="item"></table>
          <table tableName="inventory"></table>
          <table tableName="sequence"></table>

    </context>
</generatorConfiguration>